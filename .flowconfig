[ignore]
.*\.config\.js
.*\.spec\.js
.*/dist/*
.*/scripts/*
.*/cypress/*

[include]
../lib

[libs]
../lib/flow-typed
./flow-typed

[lints]
deprecated-utility=off
nonstrict-import=error
unsafe-getters-setters=error
untyped-import=error
untyped-type-import=error

[options]
emoji=true
module.name_mapper='^@grailbio\/\(.*\)$' -> '<PROJECT_ROOT>/../\1/src'
module.name_mapper.extension='scss' -> '<PROJECT_ROOT>/CSSModule.js.flow'
module.file_ext=.js
module.file_ext=.jsx
