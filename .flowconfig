[ignore]
.*\.spec\.js

[include]
../lib

[libs]
../lib/flow-typed
./flow-typed

[lints]
nonstrict-import=error
unsafe-getters-setters=error
untyped-import=error
untyped-type-import=error

[options]
emoji=true
module.name_mapper='^@grail\/\(.*\)$' -> '<PROJECT_ROOT>/../\1'
module.name_mapper.extension='scss' -> '<PROJECT_ROOT>/CSSModule.js.flow'
module.file_ext=.js
module.file_ext=.jsx
